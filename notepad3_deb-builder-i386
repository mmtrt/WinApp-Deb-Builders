#!/usr/bin/env bash
#
# Author:
#   mmtrt (Taqi Raza) [mttalpur@gmail.com]
#
# Description:
#   Script to get & build windows software to DEBIAN package
#
# Date: Jan 13 2018
#
spinner()
{
clear
tput civis
local pid=$1
local delay=0.175
local spinstr='|/-\'
local infotext=$2
while [ "$(ps a | awk '{print $1}' | grep $pid)" ]; do
local temp=${spinstr#?}
printf " [%c] %s" "$spinstr" "$infotext"
local spinstr=$temp${spinstr%"$temp"}
sleep $delay
printf "\b\b\b\b\b\b"
for i in $(seq 1 ${#infotext}); do
printf "\b"
done
done
printf " \b\b\b\b"
clear
}
sleep 3000 &
spinner $! "Working..." &
basedir="$(dirname $(readlink -f $0))"
pkgname="notepad3"
pkgdir="notepad3/"
permisdir="./notepad3/usr/share/notepad3"
chkzp="$(find -iname 'Notepad3_*.zip' | wc -l)"
getpkgver="$(ls|grep zip|sed 's/^Notepad3_//'|sed 's|.zip||')"
pkgver="$getpkgver"
contf=$pkgname/DEBIAN/control
scp=$pkgname/usr/share/$pkgdir
icof=$pkgname/usr/share/$pkgname/$pkgname.desktop
copyf=$pkgname/usr/share/doc/$pkgname/copyright
lintf=$pkgname/usr/share/lintian/overrides/$pkgname

if [ $chkzp -eq 0 ]; then
down="$(wget https://www.rizonesoft.com/downloads/notepad3 &> /dev/null)"
mv notepad3 download
getver="$(cat download | grep '<td>3.' | sed 's|<td>||;s|</td>||')"
ver=$getver
wget --accept "*.zip" --content-disposition --trust-server-names "https://www.rizonesoft.com/download/3943/" &> /dev/null
chkzp="$(find -iname 'Notepad3_*.zip' | wc -l)"
if [ $chkzp -eq 1 ]; then
getpkgver="$(ls|grep zip|sed 's/^Notepad3_//'|sed 's|.zip||')"
pkgver="$getpkgver"
7z x Notepad3_$pkgver.zip &> /dev/null
7z x -x'!Changes.txt' -x'!License.txt' -x'!Readme.txt' -x'!Notepad3.ini' -o"tmp/${pkgname}" Notepad3_${ver}_x86.zip &> /dev/null
rm Notepad3_${ver}_x64.zip
rm Notepad3_${ver}_x86.zip
cd "tmp/$pkgname"
cat > rawpng <<'EOF'

EOF
base64 -d rawpng > $pkgname.png
rm rawpng
cd $basedir
mkdir -p $pkgname/DEBIAN
mkdir -p $pkgname/usr/bin
mkdir -p $pkgname/usr/share/applications
mkdir -p $pkgname/usr/share/doc/$pkgname
mkdir -p $pkgname/usr/share/lintian/overrides
mkdir -p $pkgname/usr/share/pixmaps
mv tmp/$pkgname ./$pkgdir/usr/share/
rmdir tmp
cat > $scp$pkgname <<'EOF1'
#! /bin/bash

progName="notepad3"
progRealPath="/usr/share/$progName"
progHome="$HOME/.$progName"
progBin="$progName.exe"

if [ ! -d $progHome ]; then
mkdir $progHome
echo "[Notepad3]
[Settings]
SaveSettings=1
SaveRecentFiles=1
PathNameFormat=1
;Favorites=.\Favorites\
Favorites=
TabsAsSpaces=0
TabWidth=4
MarkLongLines=1
LongLinesLimit=80
LongLineMode=1
LoadASCIIasUTF8=0
FixLineEndings=0
ToolbarButtons=1 2 4 3 0 5 6 0 7 8 9 0 10 11 0 12 0 24 0 22 23 0 13 14 0 15 0 25 0 17
[Settings2]
AutoReloadTimeout=
DateTimeLong=
DateTimeShort=
DefaultDirectory=
DefaultExtension=
filebrowser.exe=
FileCheckInverval=
FileDlgFilters=
FileLoadWarningMB=
MarkOccurrencesMaxCount=
MultiFileArg=
NoCGIGuess=
NoFadeHidden=
NoFileVariables=
NoHTMLGuess=
PortableMyDocs=
OpacityLevel=
RelativeFileMRU=1
ReuseWindow=0
SciDirectWriteTech=
SciFontQuality=
SimpleIndentGuides=
SingleFileInstance=1
ShellAppUserModelID=Notepad3
ShellUseSystemMRU=1
StickyWindowPosition=
TimeStamp=
UseOldStyleBraceMatching=
WebTemplate1=https://google.com/search?q=%s
WebTemplate2=https://en.wikipedia.org/w/index.php?search=%s
;ExtendedWhiteSpaceChars=:
[Recent Files]
[Recent Find]
[Recent Replace]
[Styles]
Use2ndDefaultStyle=0" > $progHome/Notepad3.ini
fi

# Symlink stuff
mkdir -p "$progHome" || exit 1
# Delete broken symlinks
find -L "$progHome" -type l -delete
# Update existing symlinks, add new symlinks
cp -urs "$progRealPath/"* "$progHome" 2> /dev/null

# Switches: use -something instead of /something to avoid confusion with Unix paths
# Also convert Unix paths to Windows paths.
declare -a args

for arg; do
    if [[ "${arg:0:1}" = "-" ]]; then
        args+=("${arg/#-//}")
    else
        args+=("$(winepath -w "$arg")")
    fi
done

wine start /unix "$progHome/$progBin" "${args[@]}"
EOF1
echo "[Desktop Entry]" >> $icof
echo "Name=$pkgname" >> $icof
echo "GenericName=Text Editor" >> $icof
echo "Comment=Simple and free source code editor." >> $icof
echo "Encoding=UTF-8" >> $icof
echo "Version=$pkgver" >> $icof
echo "Type=Application" >> $icof
echo "Terminal=false" >> $icof
echo "Icon=$pkgname" >> $icof
echo "Exec=$pkgname %F" >> $icof
echo "StartupWMClass=$pkgname.exe" >> $icof
echo "Categories=Utility;TextEditor" >> $icof
echo "MimeType=text/plain;text/css;text/html;text/x-c++hdr;text/x-c++src;text/x-chdr;text/x-java;text/x-makefile;text/x-python;text/x-sh" >> $icof
echo "Notepad3 and MiniPath Copyright  2008-2016 Rizonesoft, All rights reserved." > $copyf
lintf=$pkgname/usr/share/lintian/overrides/$pkgname
echo "$pkgname: changelog-file-missing-in-native-package" >> $lintf
echo "$pkgname: binary-without-manpage" >> $lintf
echo "$pkgname: maintainer-script-ignores-errors" >> $lintf
cd ./$pkgname/usr/share/applications/
ln -s ../$pkgdir$pkgname.desktop ../applications/
cd ..
cd pixmaps
ln -s ../$pkgname/$pkgname.png ../pixmaps/
cd $basedir
cd ./$pkgname/usr/bin/
ln -s ../share/$pkgname/$pkgname ../bin/
cd $basedir
du -sh "$pkgname/usr" --block-size=KB|sort -h|grep kB|sed "s/kB//;s|$pkgname/usr||g;s|\t||" > fout
osiz="cat fout"
echo "Package: $pkgname" >> $contf
echo "Version: $pkgver" >> $contf
echo "Maintainer: mmtrt <mttalpur@gmail.com>" >> $contf
echo "Section: editors " >> $contf
echo "Architecture: i386" >> $contf
echo "Installed-Size: $($osiz)" >> $contf
echo "Priority: standard" >> $contf
echo "Conflicts: notepad3:amd64" >> $contf
echo "Replaces: notepad3:i386" >> $contf
echo "Homepage: https://www.rizonesoft.com/downloads/notepad3/" >> $contf
echo "Description: Its fast and light-weight Scintilla-based text editor with syntax highlighting.
 notepad3 is a free great text editor replacement." >> $contf
find "$pkgname/usr/share" -type f -exec chmod 644 {} \;
find "$pkgname/usr" -type d -execdir chmod 755 {} +
chmod 755 $scp$pkgname
fakeroot dpkg-deb --build $pkgname &> /dev/null
mv $pkgname.deb ${pkgname}_${pkgver}_i386.deb
rm fout
rm Notepad3_${pkgver}.zip
rm -rf $pkgname
rm download
tput cnorm
clear
kill $!
wait $! 2>/dev/null
fi
fi

