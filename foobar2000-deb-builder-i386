#!/usr/bin/env bash
#
# Author:
#   mmtrt (Taqi Raza) [mttalpur@gmail.com]
#
# Description:
#   Script to get & build windows software to DEBIAN package
#
# Date: Jan 13 2018
#
spinner()
{
clear
tput civis
local pid=$1
local delay=0.175
local spinstr='|/-\'
local infotext=$2
while [ "$(ps a | awk '{print $1}' | grep $pid)" ]; do
local temp=${spinstr#?}
printf " [%c] %s" "$spinstr" "$infotext"
local spinstr=$temp${spinstr%"$temp"}
sleep $delay
printf "\b\b\b\b\b\b"
for i in $(seq 1 ${#infotext}); do
printf "\b"
done
done
printf " \b\b\b\b"
clear
}
sleep 3000 &
spinner $! "Working..." &
basedir="$(dirname $(readlink -f $0))"
pkgname="foobar2000"
pkgdir="foobar2000/"
permisdir="./foobar2000/usr/share/foobar2000"
chkexe="$(find -iname 'foobar2000*.exe' | wc -l)"
getpkgver="$(ls|grep exe|sed 's/^foobar2000_v//;s/.exe//')"
pkgver="$getpkgver"
contf=$pkgname/DEBIAN/control
scp=$pkgname/usr/share/$pkgdir
icof=$pkgname/usr/share/$pkgname/$pkgname.desktop
copyf=$pkgname/usr/share/doc/$pkgname/copyright
lintf=$pkgname/usr/share/lintian/overrides/$pkgname

if [ $chkexe -eq 0 ]; then
getexe="$(wget https://www.foobar2000.org/download -nH --cut-dirs=3 -r -l 2 -A exe -R '*beta*.exe' &> /dev/null)"
chkexe="$(find -iname 'foobar2000*.exe' | wc -l)"
if [ $chkexe -eq 1 ]; then
getpkgver="$(ls|grep exe|sed 's/^foobar2000_v//;s/.exe//')"
pkgver="$getpkgver"
7z x "${pkgname}_v${pkgver}.exe" -x'!$PLUGINSDIR' -x'!$R0' -x'!icons' \
    -x'!foobar2000 Shell Associations Updater.exe' -x'!uninstall.exe' -o"tmp/${pkgname}" &> /dev/null
touch "tmp/${pkgdir}portable_mode_enabled"
cd "tmp/$pkgname"
cat > rawpng <<'EOF'

EOF
base64 -d rawpng > $pkgname.png
rm rawpng
cd $basedir
mkdir -p $pkgname/DEBIAN
mkdir -p $pkgname/usr/bin
mkdir -p $pkgname/usr/share/applications
mkdir -p $pkgname/usr/share/doc/$pkgname
mkdir -p $pkgname/usr/share/lintian/overrides
mkdir -p $pkgname/usr/share/pixmaps
mv tmp/$pkgname ./$pkgdir/usr/share/
rmdir tmp
cat > $scp$pkgname <<'EOF1'
#! /bin/bash

progName="foobar2000"
progRealPath="/usr/share/$progName"
progHome="$HOME/.$progName"
progBin="$progName.exe"
progUiFix="$progHome/configuration/foo_ui_std.dll.cfg"

# Symlink stuff
mkdir -p "$progHome" || exit 1
# Delete broken symlinks
find -L "$progHome" -type l -delete
# Update existing symlinks, add new symlinks
cp -urs "$progRealPath/"* "$progHome" 2> /dev/null
# fix ui glitch
rm "$progUiFix"

# Switches: use -something instead of /something to avoid confusion with Unix paths
# Also convert Unix paths to Windows paths.
declare -a args

for arg; do
    if [[ "${arg:0:1}" = "-" ]]; then
        args+=("${arg/#-//}")
    else
        args+=("$(winepath -w "$arg")")
    fi
done

wine start /unix "$progHome/$progBin" "${args[@]}"
EOF1
echo "[Desktop Entry]" >> $icof
echo "Name=$pkgname" >> $icof
echo "GenericName=Audio player" >> $icof
echo "Comment=Simple and powerful audio player." >> $icof
echo "Actions=PlayPause;Stop;Next;Prev;Random;Config" >> $icof
echo "Encoding=UTF-8" >> $icof
echo "Version=$pkgver" >> $icof
echo "Type=Application" >> $icof
echo "Terminal=false" >> $icof
echo "Icon=$pkgname" >> $icof
echo "Exec=$pkgname -immediate %F" >> $icof
echo "StartupWMClass=$pkgname.exe" >> $icof
echo "Categories=AudioVideo;Audio;Player;" >> $icof
echo "MimeType=audio/aac;audio/x-ape;audio/basic;audio/mp4;audio/mpeg;audio/mpegurl;audio/vorbis;audio/x-flac;audio/x-mp2;audio/x-mp3;audio/x-mpegurl;audio/x-ms-wma;audio/x-oggflac;audio/x-speex;audio/x-vorbis;audio/x-wav;audio/m3u;audio/x-aifc;audio/x-aiffc;audio/x-aiff;audio/x-musepack;audio/x-wavpack;x-content/audio-player;audio/x-matroska;audio/x-vorbis+ogg;" >> $icof
echo "" >> $icof
echo "[Desktop Action PlayPause]" >> $icof
echo "Name=Play/Pause" >> $icof
echo "Exec=foobar2000 -playpause" >> $icof
echo "" >> $icof
echo "[Desktop Action Stop]" >> $icof
echo "Name=Stop" >> $icof
echo "Exec=foobar2000 -stop" >> $icof
echo "" >> $icof
echo "[Desktop Action Next]" >> $icof
echo "Name=Next" >> $icof
echo "Exec=foobar2000 -next" >> $icof
echo "" >> $icof
echo "[Desktop Action Prev]" >> $icof
echo "Name=Prev" >> $icof
echo "Exec=foobar2000 -prev" >> $icof
echo "" >> $icof
echo "[Desktop Action Random]" >> $icof
echo "Name=Random" >> $icof
echo "Exec=foobar2000 -rand" >> $icof
echo "" >> $icof
echo "[Desktop Action Config]" >> $icof
echo "Name=Preferences" >> $icof
echo "Exec=foobar2000 -config" >> $icof
echo "Copyright Â© 2001-2017, Peter Pawlowski" > $copyf
lintf=$pkgname/usr/share/lintian/overrides/$pkgname
echo "$pkgname: changelog-file-missing-in-native-package" >> $lintf
echo "$pkgname: binary-without-manpage" >> $lintf
echo "$pkgname: maintainer-script-ignores-errors" >> $lintf
cd ./$pkgname/usr/share/applications/
ln -s ../$pkgdir$pkgname.desktop ../applications/
cd ..
cd pixmaps
ln -s ../$pkgname/$pkgname.png ../pixmaps/
cd $basedir
cd ./$pkgname/usr/bin/
ln -s ../share/$pkgname/$pkgname ../bin/
cd $basedir
du -sh "$pkgname/usr" --block-size=KB|sort -h|grep kB|sed "s/kB//;s|$pkgname/usr||g;s|\t||" > fout
osiz="cat fout"
echo "Package: $pkgname" >> $contf
echo "Version: $pkgver" >> $contf
echo "Maintainer: mmtrt <mttalpur@gmail.com>" >> $contf
echo "Section: sound" >> $contf
echo "Architecture: i386" >> $contf
echo "Installed-Size: $($osiz)" >> $contf
echo "Priority: standard" >> $contf
echo "Replaces: foobar2000:i386" >> $contf
echo "Homepage: http://www.foobar2000.org" >> $contf
echo "Description: Music player foobar2000.
 foobar2000 is an advanced freeware audio player for the Windows platform." >> $contf
find "$pkgname/usr/share" -type f -exec chmod 644 {} \;
find "$pkgname/usr" -type d -execdir chmod 755 {} +
chmod 755 $scp$pkgname
fakeroot dpkg-deb --build $pkgname &> /dev/null
mv $pkgname.deb ${pkgname}-${pkgver}-i386.deb
rm fout
rm ${pkgname}_v${pkgver}.exe
rm -rf $pkgname
tput cnorm
clear
kill $!
wait $! 2>/dev/null
fi
fi
